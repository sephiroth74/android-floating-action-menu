<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<objectAnimator
			android:duration="@android:integer/config_shortAnimTime"
			android:propertyName="translationZ"
			android:valueFrom="@dimen/floating_action_item_elevation_normal"
			android:valueTo="@dimen/floating_action_item_elevation_pressed"
			android:valueType="floatType" />
	</item>
	<item>
		<objectAnimator
			android:duration="@android:integer/config_shortAnimTime"
			android:propertyName="translationZ"
			android:valueFrom="@dimen/floating_action_item_elevation_pressed"
			android:valueTo="@dimen/floating_action_item_elevation_normal"
			android:valueType="floatType" />
	</item>
</selector>